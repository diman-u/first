    background: url(../img/menu_cross.svg) center center no-repeat;
}
.gift {
    display:inline-block;
    padding-left:30px;
    height:27px;
    line-height:27px;
    color:#FF6666;
   /* font-family:'Circe Light', sans-serif;*/

    font-size:16px;

    background: url(../img/icon_header_gift.svg) left center no-repeat;
    overflow: hidden;
}
.gift:hover {
    color:#BF4C4C;
    background: url(../img/icon_header_gift_hover.svg) left center no-repeat;
}
.right a {
    display:inline-block;
    padding-left:35px;
    height:27px;
    line-height:27px;
    color:#666465;
    font-size:20px;
    overflow: hidden;
    margin-left:30px;
}
.right a:hover {
    color:#1B75BB;
}
.map {
    background: url(../img/icon_header_map.svg) left center no-repeat;
}
.metro {
    background: url(../img/icon_header_metro.svg) left center no-repeat;
}
.right .phone {
    background: url(../img/icon_header_phone.svg) left center no-repeat;
}
.right .check {
    background: url(../img/icon_header_check.svg) left center no-repeat;
}
.buy {
    background: url(../img/icon_header_card.svg) left center no-repeat;
}
.facebook {
    width:27px;
    height:27px;
    background: url(../img/icon_fb.svg) left center no-repeat;
    background-size:27px;
    margin-right:12px;
}
.facebook:hover {
    background-image: url(../img/icon_fb_hover.svg);
}
.instagram {
    width:27px;
    height:27px;
    background: url(../img/icon_insta.svg) left center no-repeat;
    background-size:27px;
    margin-right:12px;
}
.instagram:hover {
    background-image: url(../img/icon_insta_hover.svg);
}
.soc-bottom {
    display: flex;
    justify-content: center;
    margin-top: 30px;
    margin-bottom: 30px;
}
.soc-bottom.soc-bottom-left {
    justify-content: flex-start;
}
@media only screen and (max-width: 1230px) {
