@media (min-width: 1200px) {
    .container {
        width: 1210px;
    }
}
.cards-holder {
    position: relative;
}
.scroll {
    width: 23px;
    align-self: flex-start;
    margin-top: 500px;
}
.cards-holder {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.cards-holder-one,
.cards-holder-two {
    max-width: 48%;
}
.cards-holder-wide {
    width:100%;
    margin: 0 auto;
    margin-bottom:50px;
}
.floating {
    margin-top:50px;
}
@media only screen and (max-width: 767px) {
    .cards-holder{
        /*width:564px;*/
        width:100%;
        display: flex;
        flex-wrap: wrap;
        flex-direction: column;
    }
    .cards-holder-one,
    .cards-holder-two {
        max-width: 100%;
    }

    .relax {
        order: 1;
    }
    .floating {
        order: 2;
        margin-top:0;
    }
    .pregnancy {
        order: 3;
    }
    .nutrition {
        order: 4;
    }
    .beauty {
        order: 5;
    }
    .wellness {
        order: 6;
    }
    .recovery {
        order: 7;
    }
    .recovery_text {
        order: 8;
    }
}
.card {
    font-size: 35px;
    line-height: 40px;
    margin-bottom: 30px;
    width: 100%;
    color: #aeadae;
    position:relative;
}
.card .title-nobg {
    font-size: 35px;
    line-height: 40px;
    color: #00C0F3;
    margin-bottom: 0;
}
.card .title-nobg:before {
    content: "";
    display: block;
    margin-top: 75px;
}
.imgheader {
    position: relative;
    overflow: hidden;
}
.imgheader img {
    width: 100%;
}
.card.bg .imgheader:after {
    position: absolute;
    bottom: 0;
    content: "";
    width: 100%;
    height: 100%;
    display: block;
    background: rgba(0,0,0,0.3) url(../img/wave.svg) repeat-x 50% 100%;
    background-size: 105%;
}
.card.bg h4 {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate( -50%, -60%);
    text-align: center;
    font-size: 49px;
    line-height: 41px;
    font-weight: normal;
    color: white;
    z-index: 1;

}
.card strong, .card a {
    color: #55c2f0;
}
.under-text {
    background: #36BCEE;
    text-align:center;
    width:100%;
    padding: 75px 45px 45px 45px;
}
.recovery .under-text {
    background: #757574;
    padding-bottom: 15px;
}
.recovery .imgheader:after {
    left: 10000px;
}
.card.bg .imgheader:before {
    position: absolute;
    bottom: 0;
    content: "";
    width: 100%;
    height: 100%;
    display: block;
    background: rgba(0,0,0,0.3);
}
.recovery .more:hover {
    background: none;
}
.recovery .center-but{
    margin-bottom: 20px;
}
.recovery .center-but:after {
    content: url(../img/icure_logo_white.svg);
    display: block;
    margin-top: 30px;
}

.card.bg p, .card.bg a {
    font-size:25px;
    line-height:32px;
    font-weight: normal;
    font-family: 'Circe',sans-serif;
    color: white;
    text-align: center;
}
.card p {
/*    margin-bottom:50px;*/
    word-wrap:break-word;
}

.card a:active, a.more:active,.callme input[type="submit"]:active {
    background-color:#1B75BB;
    color:white;
    text-decoration:none;
}
.card a:hover,.card a:focus, .more:hover, .more:focus,.callme input[type="submit"]:hover,.callme input[type="submit"]:focus {
     background-color:#00C0F3;
     color:white;
     text-decoration:none;
}
.card.bg a {
    border: 2px solid white;
}
.card i {
    text-decoration: underline;
    font-style: normal;
}
.card.bg {
    margin-top: 10px;
}
.card.bg.wide {
    width:100%;
    height:auto;
    /*background-size: 100% 50%, auto, cover ;
    background-position: center bottom, center bottom, center bottom;*/
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;

}

.column-break {
    -webkit-column-break-before: always;
    break-before: column;
}

.imgheader {
    position: relative;
}
.imgheader img {
    width: 100%;
}
.wide-card.bg .imgheader:after {
    position: absolute;
    bottom: 0;
    content: "";
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.3);
}
.wide-card .wide-text {
    position: absolute;
    top: 40%;
    left: 50%;
    transform: translate( -50%, -40%);
    width: 90%;
    text-align: center;
    color: white;
    z-index: 1;
}
.wide-card.bg .video .wide-text h4:after {
    content: "";
    position: absolute;
    width: 50px;
    height: 50px;
    top: 100%;
    left: 50%;
    background: url(../img/play.png) no-repeat;
    background-size: cover;
}


.wide-card.bg h4 {
    /*font-size:40px;*/
    font-size:4rem;


    /*font-family: 'Circe Bold',sans-serif;*/
    font-weight:bold;
}
.wide-card.bg p {
    font-size:35px;
    /*font-family: 'Circe',sans-serif;*/


}
