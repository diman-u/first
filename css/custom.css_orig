/********card styles*******/
img {
    max-width: 100%;
}
body {
    font-family: 'Circe', sans-serif;
}
.card,
.wide-card.nobg h4,
.right a,
.disclaimer_text {
    font-family: 'Circe Light',sans-serif;
}
