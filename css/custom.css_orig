                /* Floating page */
.imgheader-wide {
    position: relative;
    overflow: hidden;
}
.imgheader-wide::before {
    position: absolute;
    bottom: 0;
    content: "";
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.3);
}
.imgheader-wide .wide-text {
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    height: 100%;
}
.imgheader-wide .wide-text h4 {
    position: static;
    transform: none;
}
.imgheader-wide .tick {
    margin-bottom: 0;
}
.imgheader-wide img {
    width: 100%;
}
.imgheader-wide .tick img {
    width: 50px;

}
.imgheader-wide .wide-text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 90%;
}
.imgheader-wide .wide-text .tick a:link,
.imgheader-wide .wide-text .tick a:visited,
.imgheader-wide .wide-text .tick a:hover,
.imgheader-wide .wide-text .tick a:active{
    border: none;
    background: none;
}



