/********header styles*******/
header {
    position: relative;
    height: 100%;
    margin: 0 auto;
    margin-top:61px;
    margin-bottom:32px;
    /*padding: 0 56px;*/
    width:100%;
    text-align:center;
    /*background-color:#00bbff;*/
}
.header-holder{
    width:100%;
    margin: 0 auto;
    position: relative;
}
.logo {
    position: absolute;
    left: 40%;
    margin: 11px auto 0 auto;
    width: 150px;
    height: 102px;
}
